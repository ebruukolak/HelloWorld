
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel</title>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>


    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/zabuto_calendar.css" rel="stylesheet" />
    <link href="~/Scripts/js/gritter/css/jquery.gritter.css" rel="stylesheet" />
    <link href="~/Content/lineicons/style.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/style-responsive.css" rel="stylesheet" />
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet" />


    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="http://content.jwplatform.com/libraries/Itohe0UC.js"></script>

</head>
<body>

    <section id="container">
        <header class="header black-bg">
            <a href="#" class="logo"><b>Admin Panel</b></a>
        </header>
        <section id="main-content">
            <section class="wrapper">

                <div class="row">
                    <div class="col-lg-9 main-chart">
                        <div class="row mt">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </section>
        </section>
    </section>




    <script src="~/Scripts/js/jquery.js"></script>
    <script src="~/Scripts/js/jquery-1.8.3.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Scripts/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Scripts/js/jquery.scrollTo.min.js"></script>

    <script src="~/Scripts/js/jquery.nicescroll.js" type="text/javascript"></script>

    <script src="~/Scripts/js/jquery.sparkline.js"></script>
    <script src="~/Scripts/js/common-scripts.js"></script>
    <script src="~/Scripts/js/gritter/js/jquery.gritter.js"></script>
    <script src="~/Scripts/js/gritter-conf.js"></script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)




</body>
</html>
